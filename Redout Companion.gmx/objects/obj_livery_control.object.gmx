                text = ini_read_string(string(k-1),string(j+(6*i)),"·")
                
                if text = "T"
                    draw_set_color(make_color_rgb(100,255,100))
                else if text = "W"
                    draw_set_color(make_color_rgb(255,100,100))
                else
                    draw_set_color(c_white)
                
                draw_text(centerx+(k*w2),y+yoffset+(j*h),text)
                
            }
            ini_close()
        }
        
        if global.color_mode = 2
        {
            ini_open("livery_count")
            
