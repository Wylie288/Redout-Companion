        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Plat Times/Names
draw_set_font(font25)
draw_set_color(c_white)

length = global.plat_time[global.craft,0]

y = 130

w = 0
for (i=page+1; i&lt;=length ;i++)
{
    s = string_width(global.plat_time[global.craft,i])/2
    if i &lt; page+11
        if s &gt; w
            w = s
}

for (i=page+1; i&lt;=length ;i++)
{
    if i &lt; page+11
    {
        ini_open("Plats")
            if ini_read_real(string(global.craft),global.plat_time[global.craft,i],0) = 3
                draw_set_color(make_color_rgb(179,76,49))
            else
                draw_set_color(c_white)
        ini_close()   
        
        draw_set_halign(fa_right)
        draw_text(640+w,y+3,global.plat_time[global.craft,i])     
            
        draw_set_halign(fa_left)
        draw_text(640+w+8,y+3,global.plat_time[global.craft,i+length])
        y += 58
    }
}      
</string>
          </argument>
        </arguments>
      </action>
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
