          <argument>
            <kind>1</kind>
            <string>///Other Text
w2 = 0
for (i=page+1; i&lt;=length ;i++)
{
    s = string_width(global.plat_time[global.craft,i+length])
    if i &lt; page+11
        if s &gt; w2
            w2 = s
}

ini_open("Plats")
total = 0
count = 0
class = 0
ctotal = 0
excluded = 0

for (i=1; i&lt;=4 ;i++)
{
    length2 = global.plat_time[i,0]
    for (j=1; j&lt;=length2 ;j++)
    {
        total += 1
        if ini_read_real(string(i),global.plat_time[i,j],0) = 2
        {
            total -= 1
            excluded += 1
        }
        if ini_read_real(string(i),global.plat_time[i,j],0) = 1
            count += 1
        if (i = global.craft) 
        {
            ctotal += 1
            if ini_read_real(string(i),global.plat_time[i,j],0) = 2
                ctotal -= 1
            if (ini_read_real(string(i),global.plat_time[i,j],0) = 1)
                class += 1
        }
    }
}
ini_close()

draw_set_halign(fa_center)

dist = (640-(w+48+16))/2
draw_text(dist,100,"Total: "+string(count)+"/"+string(total))

draw_set_font(font21)
draw_text(dist,140,"Class"+" "+string(global.craft)+": "+string(class)+"/"+string(ctotal))

draw_set_font(font16)
draw_text(dist,175,"Exluded: "+string(excluded))

draw_set_font(font25)
dist = (640-(w+8+w2))/2
draw_text(1280 - dist,100,"Page: "+string((page/10)+1)+"/"+string(ceil(length/10)))
</string>
