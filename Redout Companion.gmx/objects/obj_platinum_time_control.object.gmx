          <argument>
            <kind>1</kind>
            <string>///Create Toggles
if (global.craft != old_craft) or (page != old_page)
{
    y = 130
    if (global.craft != old_craft)
    page = 0
    
    old_craft = global.craft
    old_page = page
    if instance_exists(obj_plat_toggle)
        with (obj_plat_toggle)
            instance_destroy()
            
    length = global.plat_time[global.craft,0]
    if length &gt; 10
    {
        obj_platinum_next.visible = 0
        obj_platinum_prev.visible = 0
        
        if length - page - 10 &gt; 0
            obj_platinum_next.visible = 1
        if page &gt; 0
            obj_platinum_prev.visible = 1 
    }
    else
    {
        obj_platinum_next.visible = 0
        obj_platinum_prev.visible = 0
    }
           
    w = 0                   
    for (i=page+1; i&lt;=length ;i++)
    {
        s = string_width(global.plat_time[global.craft,i])/2
        if i &lt; page+11
            if s &gt; w
                w = s
    }
    
    x_toggle = 640 - (w + 48 + 16)       
    for (i=page+1; i&lt;=length ;i++)
    {
        if i &lt; page+11
        {
            with(instance_create(x_toggle,y+5,obj_plat_toggle))
                i = other.i
            y += 58
        }
    }       
}
else
{
old_craft = global.craft
old_page = page
}
</string>
